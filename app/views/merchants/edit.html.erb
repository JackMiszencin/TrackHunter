<%- model_class = Merchant -%>
<div class = "center_block">
	<h2>Editing Merchant</h2>
	<% if notice != nil %>
		<h5><%= notice %></h5>
	<% end %>
	<div id = "form_holder">
		<%= render 'form' %>
		<%- if notice != nil %>
			<br />
			<%= notice %>
			<br />
		<%- else %>
		<%- end %>
	</div>
</div>
